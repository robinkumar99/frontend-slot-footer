  },
  "devDependencies": {
    "@edx/frontend-component-footer": "^14.0.0",
    "@edx/browserslist-config": "^1.1.1",
    "@openedx/frontend-build": "^14.0.3",
    "core-js": "3.37.1",
    "glob": "7.2.3",
